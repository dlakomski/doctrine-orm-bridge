{
    "name": "dlakomski/doctrine-orm-bridge",
    "description": "Doctrine ORM bridge for using command and event buses",
    "keywords": [
        "Doctrine",
        "event bus",
        "command bus"
    ],
    "homepage": "http://github.com/SimpleBus/DoctrineORMBridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Cliff Odijk",
            "email": "cliff@jcid.nl"
        },
        {
            "name": "Ruud Kamphuis",
            "homepage": "https://github.com/ruudk"
        },
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "dlakomski/message-bus": "^6.4",
        "doctrine/dbal": "^3.8.2|^4.0",
        "doctrine/orm": "^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.11",
        "phpunit/phpunit": "^11.5.46",
        "pimple/pimple": "^3.5",
        "symfony/cache": "^6.4.23 || ^7.4",
        "symfony/phpunit-bridge": "^6.4.23 || ^7.4"
    },
    "autoload": {
        "psr-4": {
            "SimpleBus\\DoctrineORMBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleBus\\DoctrineORMBridge\\Tests\\": "tests"
        }
    },
    "suggest": {
        "dlakomski/symfony-bridge": "Bridge for using command buses and event buses with Symfony"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "conflict": {
        "doctrine/dbal": "<2.13.3",
        "doctrine/persistence": "<2.2.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
